function healself() {
    while (Player.Hits() < Player.MaxHits()) {
        Orion.BandageSelf();
        Orion.Wait(2000);
        Orion.WaitJournal('You put the bloody|You apply the bandages', Orion.Now(), Orion.Now() + 5000, 'my|sys')
    }

}

function AutoLoot() {
    Orion.Say('I love you');
}

function 开银行() {
    Orion.Say('BANK');
    if (Orion.WaitForGump(1000)) {
        var gump0 = Orion.GetGump('last');
        if ((gump0 !== null) && (!gump0.Replayed()) && (gump0.ID() === '0x00000070')) {
            gump0.Select(Orion.CreateGumpHook(1));
            Orion.Wait(100);
        }
    }
}