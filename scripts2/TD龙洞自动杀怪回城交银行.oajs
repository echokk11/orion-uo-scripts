function 存银行() {
    Orion.Wait(500);
    解除幻术();
    Orion.Wait(500);
    Orion.UseObject(0x40002A81);//随身包
    Orion.Wait(500);
    Orion.WalkTo(3686, 2517);
    Orion.Wait(500);
    Orion.UseObject(0x40A98685);//cundongxibaobao
    Orion.Wait(500);
    开银行();
    放东西到银行();
    Orion.Wait(10000);
    放东西到银行();
    Orion.Wait(20000);
    DragonA();
}

function 放东西到银行() {
    var box = 0x400018CA;  //银行里放物品的容器
    var igitems = Orion.FindType('0x0F0B|0x09B0|0x0E7D|0x0490|0x01FB|0x0DF2|0x0F07|0x12AB|0x1F15|0x13FC|0x1F17|0x0EFE|0x0F51|0x0F88|0x0F85|0x0F8C|0x0F8D|0x0F84|0x0E2E|0x100E|0x09B0|0x1F14|0x2252|0x0EFA|0x14EE|0x1BFB|0x0E21|0x0E22|0x0F7B|0x0F7A|0x0F86', -1, 0x4000956E);   //屏蔽随身包里的物品种类加这里
    igitems.forEach(function (item) {
        Orion.Ignore(item);
    });
    var items = Orion.FindTypeEx(-1, -1, 0x4000956E);
    items.forEach(function (itemb) {
        Orion.MoveItem(itemb.Serial(), -1, box);
        Orion.Wait(800);
    });
}

function 解除幻术() {
    if (Orion.FindType('0x0E21', '0x0532|0x0000', 'backpack', 'item').length == 0)  //判断背包绷带颜色是否变了。要是中幻术

    {
        Orion.CharPrint(self, 0x0026, '===中幻术==');  //' + newDrop.length + '

        if (Orion.FindType('0x0F07', '0xFFFF', 'backpack', 'item').length > 0)   //是否有药水
        {
            Orion.UseType("0x0F07", "0xFFFF");

            Orion.CharPrint('self', 0x0026, '**药水解除幻术成功!!!!**');
            Orion.Wait(1000);
            Orion.UseObject('0x4000EC90');
        }  //自己随身包的ID
    }
}

